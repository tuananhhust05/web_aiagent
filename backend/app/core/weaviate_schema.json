{
  "class": "DataAiSaleCoach",
  "description": "Vectorized text chunks from documents",
  "vectorizer": "none",
  "properties": [
    {
      "name": "user_id",
      "dataType": ["text"],
      "description": "Owner of the document"
    },
    {
      "name": "doc_id",
      "dataType": ["text"],
      "description": "ID of the original document (book)"
    },
    {
      "name": "content",
      "dataType": ["text"],
      "description": "Text content of the chunk"
    },
    {
      "name": "chunk_index",
      "dataType": ["int"],
      "description": "Order of the chunk within the document"
    }
  ]
}
